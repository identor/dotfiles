#!/bin/sh

[[ $3 ]] && DATA="--data @$PWD/$3"
[[ $MASTER_KEY ]] && MASTER_KEY_HEADER="-H X-Warp-Master-Key: $MASTER_KEY"

[[ -n $VERBOSE ]] && echo "$1: $API_URL/$2"

curl -s -X $1 \
    -H "X-Warp-Api-Key: $API_KEY" \
    $MASTER_KEY_HEADER \
    -H "Content-Type: application/json" \
    $DATA \
    $API_URL/$2 | jsonlint

